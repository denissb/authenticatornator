{
  "source": "./src",
  "destination": "./docs",
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "lint": {"enable": true},
      "coverage": {"enable": true},
      "accessor": {"access": ["public", "protected", "private"], "autoPrivate": true},
      "undocumentIdentifier": {"enable": true},
      "unexportedIdentifier": {"enable": false},
      "typeInference": {"enable": true},
      "test": {
        "source": "./src/",
        "interfaces": ["describe", "it", "context", "suite", "test"],
        "includes": ["(spec|Spec|test|Test)\\.js$"]
      }
    }
  },{
    "name": "esdoc-babel-plugin",
    "option": {
      "presets": ["stage-1"]
    }
  }, {
    "name": "esdoc-ecmascript-proposal-plugin",
    "option": {
      "all": true
    }
  }, {
    "name": "esdoc-importpath-plugin",
    "option": {
      "stripPackageName": true,
      "replaces": [
        { "from": "src/AuthClient.js", "to": "authenticatornator"},
        { "from": "^src/", "to": "authenticatornator/lib/" }
      ]
    }
  }, {
    "name": "esdoc-brand-plugin",
    "option": {
      "title": "Authenticatornator",
      "description": "this is awesome library",
      "repository": "https://github.com/morten-olsen/authclient",
      "site": "https://morten-olsen.github.io/authclient/",
      "author": "https://github.com/morten-olsen"
    }
  }, {
    "name": "esdoc-publish-html-plugin",
    "option": {
      "template": "./node_modules/esdoc-custom-theme/template"
    }
  }]
}